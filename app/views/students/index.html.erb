<table class="table table-striped table-bordered" style="text-align: center;">
  <tr>
    <th class="center">Last Name</th>
    <th class="center">First Name</th>
    <th class="center">Email</th>
    <th class="center">Phone Number</th>
    <th class="center">Full Profile</th>
  </tr>

  <% @students.each do |student| %>
    <tr>
      <td class="center"><%= student.last_name %></td>
      <td class="center"><%= student.first_name %></td>
      <td class="center"><%= student.email %></td>
      <td class="center">090-1233-2312</td>
      <td class="center"><%= link_to "Profile", "#", class: "btn btn-primary" %></td>
    </tr>
  <% end %>
</table>